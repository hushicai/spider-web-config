{
  "name": "spider-web-config",
  "version": "1.0.0",
  "description": "基于React的爬虫配置系统",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-runtime": "^6.20.0",
    "brace": "^0.10.0",
    "classnames": "^2.2.5",
    "concurrently": "^3.4.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.5",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "flux": "^3.1.2",
    "html-webpack-harddisk-plugin": "^0.0.2",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "just-wait": "^1.0.11",
    "mock-middleware": "^1.0.0",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.2.2",
    "react-ace": "^4.1.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redbox-react": "^1.3.3",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.1.1",
    "scss-loader": "^0.0.1",
    "serve-index": "^1.8.0",
    "serve-static": "^1.11.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "url-search-params": "^0.6.1",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chokidar": "^1.6.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-node-externals": "^1.5.4"
  },
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --progress --config ./webpack/webpack-client.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --progress --config ./webpack/webpack-server.config.js",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --config ./webpack/webpack-server.config.js",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "wait-for-server": "just-wait --pattern \"build/server.js\" && node ./build/server.js",
    "dev": "concurrently \"npm run watch\" \"npm run wait-for-server\"",
    "production": "concurrently \"npm run build\" \"npm run wait-for-server\"",
    "start": "cross-env NODE_ENV=development babel-node ./bootstrap.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hushicai/spider-web-config.git"
  },
